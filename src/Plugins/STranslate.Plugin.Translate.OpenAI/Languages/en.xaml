<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Url">Url</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Url_Description">Configure the OpenAI API service address.&#x0a;If the Path is not filled in, it will be automatically populated with /v1/chat/completions.&#x0a;For example, for the OpenAI API address https://api.openai.com/v1/chat/completions, the Path is /v1/chat/completions.</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_ApiKey">ApiKey</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_ApiKey_Placeholder">Please enter your apiKey</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_ApiKey_Description">Enter your OpenAI API key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Model">Select a model</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Prompt_Header">Prompt Configuration</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Prompt_Description">Select or edit the prompt template for translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Prompt_Placeholder">Select a prompt template</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_EditPrompt_ToolTip">Edit Prompt</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_EditPrompt_Content">Edit</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_AutoTransBack_Header">Back Translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_AutoTransBack_Description">Automatically perform back translation after translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_On">On</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Off">Off</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Validate">Validate</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_ValidateBtnContent">Validate</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_OpenAI_Validate_Description">Test if the API connection and configuration are correct</sys:String>

